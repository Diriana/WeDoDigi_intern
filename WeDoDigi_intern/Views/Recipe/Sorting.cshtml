@model IEnumerable<TagDb>
@{
    ViewData["Title"] = "Sorting";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Sorting</h1>
<table class="table">
    <tbody>
        @foreach (TagDb tDb in Model)
        {
            <tr>
                <td>
                    @Html.ActionLink(tDb.tagName, "SortingResult", "Recipe", new {id = tDb.id})
                </td>
            </tr>
        }
    </tbody>
</table>